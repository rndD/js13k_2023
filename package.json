{
  "name": "medieval-commerce",
  "version": "1.0.0",
  "main": "index.js",
  "devDependencies": {
    "@rollup/plugin-typescript": "8.5.0",
    "clean-css": "5.3.1",
    "copy-webpack-plugin": "9.0.1",
    "cross-env": "7.0.3",
    "ect-bin": "1.4.1",
    "google-closure-compiler": "20230206.0.0",
    "html-minifier": "4.0.0",
    "html-webpack-plugin": "5.3.2",
    "npm-run-all": "4.1.5",
    "roadroller": "2.1.0",
    "tmp": "0.2.1",
    "ts-loader": "9.2.5",
    "ts-standard": "12.0.2",
    "typescript": "4.9.5",
    "vite": "4.1.3",
    "webpack": "5.50.0",
    "webpack-cli": "4.8.0",
    "webpack-dev-server": "4.0.0"
  },
  "scripts": {
    "serve": "npm-run-all --parallel tsc vite",
    "serve-dist": "python3 -m http.server --directory dist",
